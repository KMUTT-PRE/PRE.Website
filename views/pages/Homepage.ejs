  <body>
    <%- include('../partials/topbtn') %>
    <!-- picture-main -->
    <div class="carousel-wrapper">
      <div
        id="carouselExampleSlidesOnly"
        class="carousel slide"
        data-bs-ride="carousel"
      >
        <div class="carousel-inner">
          <div class="carousel-item active" data-bs-interval="5000">
            <img
              src="../images/Homepage/Homepage-mainpic1.png"
              class="d-block w-100"
              alt="HomepagePic1"
            />
          </div>
          <div class="carousel-item" data-bs-interval="5000">
            <img
              src="../images/Homepage/Homepage-mainpic2.png"
              class="d-block w-100"
              alt="HomepagePic2"
            />
          </div>
          <div class="carousel-item" data-bs-interval="5000">
            <img
              src="../images/Homepage/Homepage-mainpic3.png"
              class="d-block w-100"
              alt="HomepagePic3"
            />
          </div>
          <div class="carousel-item" data-bs-interval="5000">
            <img
              src="../images/Homepage/Homepage-mainpic4.png"
              class="d-block w-100"
              alt="HomepagePic4"
            />
